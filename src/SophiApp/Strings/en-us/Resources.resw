&lt;actions&gt;
	&lt;action arguments="{1}" content="Download" activationType="protocol"/&gt;
	&lt;action arguments="dismiss" content="" activationType="system"/&gt;
&lt;/actions&gt;
&lt;/toast&gt;</value>
