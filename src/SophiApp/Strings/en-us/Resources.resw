  </data>
  <data name="UIModel_AppsSilentInstalling_Title" xml:space="preserve">
    <value>Enable automatic installing suggested apps</value>
  </data>
  <data name="UIModel_AppsSilentInstalling_Description" xml:space="preserve">
    <value>Windows may silently download and install some suggested apps from the Microsoft Store without any user confirmation.</value>
  </data>
  <data name="UIModel_WhatsNewInWindows_Title" xml:space="preserve">
